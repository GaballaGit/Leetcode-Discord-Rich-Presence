{
    "manifest_version": 3,
    "name": "Leetcode Discord Rich Presence",
    "description": "Show your discord friends your Leetcode grind!",
    "version": "1.0",
    "action": {
        "default_icon": "./images/leetcode-icon.jpeg"
    },
    "content_scripts": [
        {
            "js": ["./app.js"],
            "matches": [
                "https://leetcode.com/problemset/",
                "https://leetcode.com/problems/*"
            ]
        }
    ]
}